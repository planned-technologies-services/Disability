<?xml version="1.0" encoding="utf-8"?>
<dataController name="OrderDetailsExtended" conflictDetection="overwriteChanges" label="Order Details Extended" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"OrderDetailsExtended"."OrderID" "OrderID"
	,"OrderDetailsExtended"."ProductID" "ProductID"
	,"OrderDetailsExtended"."ProductName" "ProductName"
	,"OrderDetailsExtended"."UnitPrice" "UnitPrice"
	,"OrderDetailsExtended"."Quantity" "Quantity"
	,"OrderDetailsExtended"."Discount" "Discount"
	,"OrderDetailsExtended"."ExtendedPrice" "ExtendedPrice"
from "dbo"."Order Details Extended" "OrderDetailsExtended"
]]></text>
    </command>
    <command id="ProductIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="ProductID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="OrderID" type="Int32" allowNulls="false" label="Order ID" showInSummary="true" />
    <field name="ProductID" type="Int32" allowNulls="false" label="Product ID" readOnly="true" showInSummary="true" />
    <field name="ProductName" type="String" allowNulls="false" label="Product Name" length="40" showInSummary="true" />
    <field name="UnitPrice" type="Decimal" allowNulls="false" dataFormatString="c" label="Unit Price" showInSummary="true" />
    <field name="Quantity" type="Int16" allowNulls="false" label="Quantity" showInSummary="true" />
    <field name="Discount" type="Single" allowNulls="false" label="Discount" />
    <field name="ExtendedPrice" type="Decimal" dataFormatString="c" label="Extended Price" readOnly="true" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Order Details Extended">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="OrderID" columns="15" />
        <dataField fieldName="ProductID" columns="15" />
        <dataField fieldName="ProductName" columns="40" />
        <dataField fieldName="UnitPrice" columns="15" />
        <dataField fieldName="Quantity" columns="15" />
        <dataField fieldName="Discount" columns="15" />
        <dataField fieldName="ExtendedPrice" columns="15" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Order Details Extended">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Order Details Extended" flow="NewColumn">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
